<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="476.0" prefWidth="632.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.dev.genshinbuildsaver.ListMenu">
   <children>
      <Label layoutX="239.0" layoutY="44.0" text="Welcome, " textAlignment="CENTER">
         <font>
            <Font name="HYWenHei 85W" size="29.0" />
         </font>
      </Label>
      <Label layoutX="178.0" layoutY="109.0" text="Feel free to choose the list you like">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="listTView" layoutX="58.0" layoutY="164.0" prefHeight="200.0" prefWidth="517.0">
        <columns>
          <TableColumn fx:id="nameTCol" prefWidth="75.0" text="Name" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="58.0" layoutY="397.0" mnemonicParsing="false" onAction="#onAddAction" text="Add new list">
         <font>
            <Font name="HYWenHei 85W" size="17.0" />
         </font>
      </Button>
      <Button layoutX="431.0" layoutY="397.0" mnemonicParsing="false" onAction="#onDropAction" text="Drop selected">
         <font>
            <Font name="HYWenHei 85W" size="17.0" />
         </font>
      </Button>
      <Button fx:id="selectBtt" layoutX="279.0" layoutY="397.0" mnemonicParsing="false" onAction="#onSelectAction" text="Select">
         <font>
            <Font name="HYWenHei 85W" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
